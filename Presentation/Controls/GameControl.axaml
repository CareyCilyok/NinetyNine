<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="NinetyNine.Presentation.Controls.GameControl"
             xmlns:controls="using:NinetyNine.Presentation.Controls">

   <StackPanel Orientation="Horizontal" Spacing="3" >
         <controls:FrameControl Grid.Column="0" Height="75" Width="50" VerticalContentAlignment="Center"/>
         <controls:FrameControl Grid.Column="1" Height="75" Width="50" VerticalContentAlignment="Center"/>
         <controls:FrameControl Grid.Column="2" Height="75" Width="50" VerticalContentAlignment="Center"/>
         <controls:FrameControl Grid.Column="3" Height="75" Width="50" VerticalContentAlignment="Center"/>
         <controls:FrameControl Grid.Column="4" Height="75" Width="50" VerticalContentAlignment="Center"/>
         <controls:FrameControl Grid.Column="5" Height="75" Width="50" VerticalContentAlignment="Center"/>
         <controls:FrameControl Grid.Column="6" Height="75" Width="50" VerticalContentAlignment="Center"/>
         <controls:FrameControl Grid.Column="7" Height="75" Width="50" VerticalContentAlignment="Center"/>
         <controls:FrameControl Grid.Column="8" Height="75" Width="50" VerticalContentAlignment="Center"/>
   </StackPanel>
</UserControl>
